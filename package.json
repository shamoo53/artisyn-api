{
  "name": "artisyn-api",
  "version": "1.0.0",
  "description": "Backend API for Artisyn - a decentralized protocol connecting local artisans with users",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "migrate": "prisma migrate dev",
    "seed": "ts-node src/database/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toneflix/artisyn-api.git"
  },
  "keywords": [
    "artisans",
    "starknet",
    "decentralized",
    "marketplace"
  ],
  "author": "Toneflix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toneflix/artisyn-api/issues"
  },
  "homepage": "https://github.com/toneflix/artisyn-api#readme",
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.10.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}